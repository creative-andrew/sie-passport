    <div id="small-dialog" class="zoom-anim-dialog mfp-hide">
        <h1>Entrar</h1>

        <div class="fb-button-login">
            <i class="fa fa-facebook" aria-hidden="true"></i>
            Ingresar con <strong>facebook</strong>
        </div>

        <div class="gg-button-login" style="margin-top: 5px">
            <i class="fa fa-google-plus" aria-hidden="true"></i>
            Ingresar con <strong>Google +</strong>
        </div>

        <hr class="style-eight">
        <form action="">
            <div class="form-group">
                <label for="username">Usuario</label>
                <input class="form-control" type="email" id="username" name="_username" required="required"  placeholder="Usuario">
            </div>

            <div class="form-group">
                <label for="password">Contraseña</label>
                <input class="form-control" type="password" id="password" name="_password" required="required"  placeholder="Contraseña">
            </div>


            <div class="form-group ">
                <div class="olv-pass">
                    <a href="javascript:void(0)">¿Olvidaste tu contraseña?</a>
                </div>
            </div>

            <div style="text-align: center">
                <button type="submit" class="btn submit_sie" id="_submit" name="_submit" value="{{ 'security.login.submit'|trans }}" >Entrar</button>
            </div>

        </form>



    </div>


    <div id="small-dialog2" class="zoom-anim-dialog mfp-hide">
        <h1>Registro</h1>
        <form action="">

            <div class="form-group">
                <label for="username">Email</label>
                <input class="form-control" type="email" id="email" name="_email" required="required"  placeholder="email">
            </div>
            <div class="form-group">
                <label for="password">Usuario</label>
                <input class="form-control" type="text" id="usuario_reg" name="_usuarioreg" required="required"  placeholder="Usuario">
            </div>
            <div class="form-group">
                <label for="password">Contraseña</label>
                <input class="form-control" type="password" id="password1" name="_password" required="required"  placeholder="Contraseña">
            </div>

            <div class="form-group">
                <label for="password">Repita Contraseña</label>
                <input class="form-control" type="password" id="password2" name="_password" required="required"  placeholder="Repita Contraseña">
            </div>

            <div style="text-align: center">
                <button type="submit" class="btn submit_sie" id="_reg" name="_submit">Registrarme</button>
            </div>

        </form>



    </div>




    <div class="element"></div>
    <img class="logotipe" src="/img/logotipo_l_blanco.svg" alt="">
    <a href="#first">
        <img class="scrolldown" src="/img/scrolldown.svg" alt="">
    </a>
    <!-- new slide -->
    <div class="ms-slide" data-fill-mode="fill" data-delay="10">
        <img src="/img/back1.png" data-src="/img/back5.jpg" alt="lorem ipsum dolor sit"/>
