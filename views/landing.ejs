    <div class="buscador-principal">
        <div id="buscador1">
            <i class="fa fa-chevron-down" aria-hidden="true"></i>
            <form class="searcher" method="GET" action="" role="search">

                <select name="slug_seccion"  class="selectpicker">
                    <option value="Compras">Compras</option>
                    <option value="Invierte">Invierte</option>
                    <option value="Asociate">Asociate</option>
                </select>
                <input name="search" class="search" type="text" placeholder="Ej: Regalos,Accesorios para Mascotas, Belleza"/>
            </form>
        </div>
    </div>


    <div id="small-dialog" class="zoom-anim-dialog mfp-hide">
        <h1>Entrar</h1>

