            <i class="fa fa-facebook" aria-hidden="true"></i>
            Ingresar con <strong>facebook</strong>
        </div>

        <div class="gg-button-login" style="margin-top: 5px">
            <i class="fa fa-google-plus" aria-hidden="true"></i>
            Ingresar con <strong>Google +</strong>
        </div>

        <hr class="style-eight">
        <form action="">
            <div class="form-group">
                <label for="username">Usuario</label>
                <input class="form-control" type="email" id="username" name="_username" required="required"
                       placeholder="Usuario">
            </div>

            <div class="form-group">
                <label for="password">Contraseña</label>
                <input class="form-control" type="password" id="password" name="_password" required="required"
                       placeholder="Contraseña">
            </div>


            <div style="text-align: center">
                <button type="submit" class="btn submit_sie" id="_submit" name="_submit"
                        value="{{ 'security.login.submit'|trans }}">Entrar
                </button>
            </div>

        </form>


    </div>
    <img class="logotipe" src="/img/logotipo_l_blanco.svg" alt="">


    <!-- new slide -->
    <div class="ms-slide" data-fill-mode="fill" data-delay="10">
        <img src="/img/back1.png" data-src="/img/back5.jpg" alt="lorem ipsum dolor sit"/>
        <div  class="ms-layer ms-caption element2"
              data-offset-x      = "300"
              data-offset-y      = "200"
              data-origin        = "tr"
